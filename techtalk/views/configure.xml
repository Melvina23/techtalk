<odoo>
    <data>
<!--district-->
        <record id="tech_district_form" model="ir.ui.view">
            <field name="name">tech.district.form</field>
            <field name="model">tech.district</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group string="district details">
                            <group>
                                <field name="name" />
                                <field name="code"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="tech_district_tree" model="ir.ui.view">
            <field name="name">tech.district.tree</field>
            <field name="model">tech.district</field>
            <field name="arch" type="xml">
                <tree>
                     <field name="name" />
                     <field name="code"/>
                </tree>
            </field>
        </record>


        <record id="tech_district_action" model="ir.actions.act_window">
            <field name="name">District</field>
            <field name="res_model">tech.district</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create New Record.
                </p>
            </field>
        </record>

<!--state-->
